# Basic_Scraper
import requests
params = {'search_keywords': 'ethereum merge','user-agent':'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/105.0.0.0 Safari/537.36'}
r = requests.post("https://www.wsj.com/", data=params)
print(r)
